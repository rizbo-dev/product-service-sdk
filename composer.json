{
  "name": "boris/product-service-sdk",
  "autoload": {
    "psr-4": {
      "Boris\\ProductServiceSdk\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Boris\\ProductServiceSdk\\Tests\\": "tests"
    }
  },
  "authors": [
    {
      "name": "Boris Evetovic",
      "email": "boris.evetovic@infostud.com"
    }
  ],
  "scripts": {
    "fix-style": "vendor/bin/php-cs-fixer fix --allow-risky=yes",
    "check-style": "vendor/bin/php-cs-fixer fix --allow-risky=yes --dry-run"
  },
  "require": {
    "symfony/http-client": "^7.0",
    "symfony/serializer": "^7.0",
    "symfony/property-access": "^7.0",
    "phpdocumentor/reflection-docblock": "^5.3"

  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "phpunit/phpunit": "^10.5",
    "phpstan/phpstan": "^1.10",
    "friendsofphp/php-cs-fixer": "^3.42",
    "symfony/var-dumper": "^7.0"
  }
}
